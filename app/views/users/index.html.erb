<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <th scope="row"><%= user['id'] %></th>
      <td><%= user['first_name'] %></td>
      <td><%= user['last_name'] %></td>
      <td><%= user['email'] %></td>
    </tr>
    <% end %>
  </tbody>
</table>
  <%= link_to 'Add New User', new_user_path, class: 'btn btn-primary'%>